{
  "name": "ongama-nft-backend",
  "version": "0.0.1",
  "description": "ONGANFTs",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "preinstall": "rm -rf /dist",
    "postinstall": "npm run build & npm run migrate:run",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "npm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm-cli": "ts-node ./node_modules/typeorm/cli.js $*",
    "migrate:win-run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:run": "npm run typeorm-cli migration:run",
    "migrate:revert": "npm run typeorm-cli migration:revert",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -d src/database/migrations -n "
  },
  "dependencies": {
    "@ethersproject/networks": "^5.6.2",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.4",
    "@sentry/node": "6.8.0",
    "async": "3.2.0",
    "aws-sdk": "2.994.0",
    "axios": "^0.26.1",
    "bignumber.js": "9.0.1",
    "cache-manager": "3.4.4",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.2",
    "csv-load-sync": "2.3.1",
    "dayjs": "1.10.4",
    "dotenv": "9.0.2",
    "eslint-config-airbnb-base": "14.2.1",
    "ethereumjs-util": "7.0.10",
    "ethers": "5.5.1",
    "express-useragent": "^1.0.15",
    "graphql-request": "^4.2.0",
    "joi": "^17.6.0",
    "lodash": "4.17.21",
    "minimist": "^1.2.6",
    "morgan": "1.10.0",
    "node-polyglot": "2.4.0",
    "pg": "8.6.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "2.1.3",
    "rimraf": "3.0.2",
    "rxjs": "^7.1.0",
    "slugify": "^1.6.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "0.2.34",
    "uuid": "8.3.2",
    "web3": "^1.7.0",
    "web3-utils": "1.5.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.5",
    "@types/express": "4.17.11",
    "@types/jest": "27.0.2",
    "@types/multer": "1.4.5",
    "@types/node": "14.17.18",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "audit-ci": "5.1.2",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "27.3.0",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "27.0.0",
    "ts-loader": "8.0.18",
    "ts-node": "10.8.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
